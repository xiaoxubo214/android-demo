<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomPersonalItemLayout">
        <attr name="tv_text" format="string"/>
        <attr name="tv_text_size" format="reference|dimension"/>
        <attr name="tv_text_color" format="reference|color"/>
        <attr name="tv_drawable_left" format="reference|integer"/>
        <attr name="iv_drawable_right" format="reference|integer"/>
        <attr name="is_enabled_center" format="boolean"/>
        <attr name="line_is" format="boolean"/>
    </declare-styleable>

    <!--loding Dialog属性-->
    <style name="indeterminate_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>


    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
</resources>
